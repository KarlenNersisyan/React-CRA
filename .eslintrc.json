{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb-base"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "error",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "linebreak-style": "off",
    "react/prop-types": "off",

    "react/react-in-jsx-scope": "off",
    "arrow-parens": "off",
    "object-curly-newline": "off"
  }
}
